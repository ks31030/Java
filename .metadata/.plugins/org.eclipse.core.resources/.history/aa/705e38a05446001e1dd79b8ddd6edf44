package com.kh.practice2.func;

import java.util.*;

public class CastingPractice2 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		double grd1, grd2, grd3;
		
		System.out.print("국어 : ");
		grd1 = sc.nextDouble();
		System.out.print("영어 : ");
		grd2 = sc.nextDouble();
		System.out.print("수학 : ");
		grd3 = sc.nextDouble();
	}

}
